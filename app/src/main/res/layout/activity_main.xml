<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="academia.projetoacademia.MainActivity">

    <ImageButton
        android:id="@+id/btnAddExercicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/gym"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="80dp"
        android:layout_marginBottom="46dp"
        android:layout_above="@+id/btnListarExercicios"
        android:layout_alignStart="@+id/btnListarExercicios" />

    <ImageButton
        android:id="@+id/btnListarExercicios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/list"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="277dp"
        android:layout_marginBottom="69dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
